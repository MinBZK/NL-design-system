{
  "name": "@nl-minbzk/storybook",
  "version": "1.0.0-alpha.0",
  "author": "Community for NL Design System",
  "description": "Storybook of MinBZK based on the NL Design System architecture",
  "license": "EUPL-1.2",
  "keywords": [
    "nl-design-system",
    "minbzk"
  ],
  "private": true,
  "repository": {
    "type": "git+https",
    "url": "https://github.com/MinBZK/NL-design-system.git",
    "directory": "packages/storybook"
  },
  "scripts": {
    "build": "storybook build --output-dir dist/ --config-dir config/",
